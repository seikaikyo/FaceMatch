{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/demo.html",
      "dest": "/public/demo.html"
    },
    {
      "src": "/app.html",
      "dest": "/public/app.html"
    },
    {
      "src": "/demo",
      "dest": "/public/demo.html"
    },
    {
      "src": "/app",
      "dest": "/public/app.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}