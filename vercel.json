{
  "version": 2,
  "name": "facematch-system",
  "builds": [
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/index.html",
      "dest": "/static/index.html"
    },
    {
      "src": "/demo.html",
      "dest": "/static/demo.html"
    },
    {
      "src": "/demo",
      "dest": "/static/demo.html"
    },
    {
      "src": "/app",
      "dest": "/static/index.html"
    },
    {
      "src": "/",
      "dest": "/static/demo.html"
    }
  ]
}